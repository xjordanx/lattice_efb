
<lsccip:busInterfaces xmlns:lsccip="http://www.latticesemi.com/XMLSchema/Radiant/ip">
    <lsccip:busInterface>
        <lsccip:name>APB_S0</lsccip:name>
        <lsccip:displayName>APB</lsccip:displayName>
        <lsccip:description>Advanced Peripheral Bus Target Device Interface</lsccip:description>
        <lsccip:busType vendor="amba.com" library="AMBA3" name="APB" version="r1p0"/>
        <lsccip:abstractionTypes>
            <lsccip:abstractionType>
                <lsccip:abstractionRef vendor="amba.com" library="AMBA3" name="APB_rtl" version="r1p0"/>
                <lsccip:portMaps>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PADDR</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_addr</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PCLK</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pclk</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PENABLE</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_penable</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PRDATA</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_prdata</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PREADY</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pready</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PRESETn</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_resetn</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PSELx</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pselx</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PSLVERR</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pslverr</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PWDATA</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pwdata</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>PWRITE</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>apb_pwrite</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                </lsccip:portMaps>
            </lsccip:abstractionType>
        </lsccip:abstractionTypes>
        <lsccip:slave>
            <lsccip:memoryMapRef memoryMapRef="efb_spi_memory_map"/>
        </lsccip:slave>
    </lsccip:busInterface>
    <lsccip:busInterface>
        <lsccip:name>INT_M0</lsccip:name>
        <lsccip:displayName>INT_O</lsccip:displayName>
        <lsccip:description>Interrupt Output</lsccip:description>
        <lsccip:busType vendor="spiritconsortium.org" library="busdef.interrupt" name="interrupt" version="1.0"/>
        <lsccip:abstractionTypes>
            <lsccip:abstractionType>
                <lsccip:abstractionRef vendor="spiritconsortium.org" library="busdef.interrupt" name="interrupt_rtl" version="1.0"/>
                <lsccip:portMaps>
                    <lsccip:portMap>
                        <lsccip:logicalPort>
                            <lsccip:name>IRQ</lsccip:name>
                        </lsccip:logicalPort>
                        <lsccip:physicalPort>
                            <lsccip:name>spi_irq</lsccip:name>
                        </lsccip:physicalPort>
                    </lsccip:portMap>
                </lsccip:portMaps>
            </lsccip:abstractionType>
        </lsccip:abstractionTypes>
        <lsccip:master/>
    </lsccip:busInterface>
</lsccip:busInterfaces>